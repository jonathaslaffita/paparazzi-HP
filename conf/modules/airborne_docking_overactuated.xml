<!DOCTYPE module SYSTEM "module.dtd">
<module name="airborne_docking_overactuated" dir="airborne_docking" task="default">
  <doc>
    <description>Airborne docking module for relative setpoint formulation and acceleration command</description>
  </doc>
  <settings>
    <dl_settings>
      <dl_settings name="AIRBORNE_DOCKING">
        <dl_setting var="relative_position_P_GAIN_X" min="-30" step="0.1" max="30" shortname="pro to vel x"/>
        <dl_setting var="relative_position_P_GAIN_Y" min="-30" step="0.1" max="30" shortname="pro to vel y"/>
        <dl_setting var="relative_position_P_GAIN_Z" min="-30" step="0.1" max="30" shortname="pro to vel z"/>
        <dl_setting var="relative_position_D_GAIN_X" min="-30" step="0.1" max="30" shortname="der to vel x"/>
        <dl_setting var="relative_position_D_GAIN_Y" min="-30" step="0.1" max="30" shortname="der to vel y"/>
        <dl_setting var="relative_position_D_GAIN_Z" min="-30" step="0.1" max="30" shortname="der to vel z"/>
        <dl_setting var="pre_docked_setpoint_X" min="-4" step="0.1" max="4"/>
        <dl_setting var="pre_docked_setpoint_Y" min="-4" step="0.1" max="4"/>
        <dl_setting var="pre_docked_setpoint_Z" min="-4" step="0.1" max="4"/>
        <dl_setting var="velocity_speed_gain" min="-4" step="0.1" max="4"/>
      </dl_settings>
    </dl_settings>
  </settings>
  <header>
    <file name="airborne_docking_overactuated.h"/>
  </header>
  <init fun="airborne_docking_init()"/>
  <!-- <periodic fun="airborne_docking_periodic()" freq="5.0" autorun="TRUE"/>  -->
  <!-- IF YOU CHANGE PERIODIC FREQUENCY ALSO CHANGE IT IT THE DERRIVATES -->
  <datalink message="REMOTE_GPS_SMALL" fun="gps_other_ac_datalink_parse_REMOTE_GPS_SMALL(buf)"/>
  <!-- <event fun="qwe()"/> -->
  <makefile>
    <file name="airborne_docking_overactuated.c"/>
  </makefile>
</module>